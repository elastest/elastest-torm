{
  "name": "elastest-torm-gui",
  "version": "1.0.0-beta5",
  "private": true,
  "description": "ElasTest TORM GUI",
  "keywords": [
    "angular",
    "components",
    "reusable"
  ],
  "scripts": {
    "e2e": "ng e2e",
    "lint": "ng lint",
    "reinstall": "rm -rf node_modules tmp deploy dist && npm i",
    "postinstall": "webdriver-manager update",
    "start-api": "cd mock-api && covalent-data &",
    "stop-api": "pkill covalent-data",
    "webdriver-update": "bash ./node_modules/.bin/webdriver-manager update",
    "test": "ng test --code-coverage --single-run",
    "ghpages-deploy": "ng build --aot --prod --env=prod --sourcemap=false",
    "start": "ng serve --proxy-config proxy.conf.json"
  },
  "engines": {
    "node": ">4.4 < 7",
    "npm": ">3"
  },
  "repository": "https://github.com/elastest/elastest-torm",
  "license": "Apache v2",
  "dependencies": {
    "@angular/animations": "7.2.0",
    "@angular/cdk": "7.2.1",
    "@angular/common": "7.2.0",
    "@angular/compiler": "7.2.0",
    "@angular/core": "7.2.0",
    "@angular/flex-layout": "7.0.0-beta.23",
    "@angular/forms": "7.2.0",
    "@angular/http": "7.2.0",
    "@angular/material": "7.2.1",
    "@angular/platform-browser": "7.2.0",
    "@angular/platform-browser-dynamic": "7.2.0",
    "@angular/platform-server": "7.2.0",
    "@angular/router": "7.2.0",
    "@covalent/core": "2.0.0",
    "@covalent/dynamic-forms": "2.0.0",
    "@covalent/highlight": "2.0.0",
    "@covalent/http": "2.0.0",
    "@covalent/markdown": "2.0.0",
    "@stomp/ng2-stompjs": "7.1.0-beta.1",
    "@swimlane/ngx-charts": "10.0.0",
    "ag-grid": "18.1.2",
    "ag-grid-angular": "19.1.2",
    "ag-grid-community": "19.1.4",
    "ngx-autosize": "1.2.4",
    "angular-tree-component": "7.0.2-beta1",
    "core-js": "2.4.1",
    "d3": "5.7.0",
    "hammerjs": "^2.0.8",
    "highlight.js": "9.13.1",
    "ng2-stomp-service": "1.2.2",
    "ng2-trim-directive": "2.3.2",
    "rxjs": "6.3.3",
    "rxjs-compat": "6.3.3",
    "showdown": "1.9.0",
    "sockjs-client": "^1.1.4",
    "time-ago-pipe": "1.3.2",
    "tslib": "^1.9.0",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.11.0",
    "@angular/cli": "7.1.4",
    "@angular/compiler-cli": "7.2.0",
    "@types/hammerjs": "2.0.36",
    "@types/jasmine": "3.3.5",
    "@types/node": "~10.12.18",
    "@types/selenium-webdriver": "3.0.14",
    "codelyzer": "~4.5.0",
    "jasmine-core": "~3.3.0",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "~3.1.4",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~2.0.0",
    "karma-coverage-istanbul-reporter": "^2.0.4",
