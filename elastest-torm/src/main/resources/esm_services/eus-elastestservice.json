   "manifest": {
      "id": "2bd62bc2-f768-42d0-8194-562924b494ff",
      "manifest_content": "version: '2.1'\nservices:\n   elastest-eus:\n      image: elastest/eus\n      environment:\n         - USE_TORM=true\n      expose:\n         - 8040\n      networks:\n         - elastest_elastest\n      volumes:\n         - /var/run/docker.sock:/var/run/docker.sock\n         - ${ET_DATA_IN_HOST}:${ET_SHARED_FOLDER}\nnetworks:\n  elastest_elastest:\n    external: true",
      "manifest_type": "docker-compose",
      "plan_id": "b4cfc681-0e28-41f0-b88c-dde69169a256",
      "service_id": "29216b91-497c-43b7-a5c4-6613f13fa0e9",
      "config": {
         "webRtcStats": {
           "type": "boolean",
           "label": "Gather WebRTC Statistics",
           "default": false
         }
      },
      "endpoints": {
         "elastest-eus": {
            "description": "W3C WebDriver standard sessions operations",
            "main": true,
            "api": [
               {
                  "protocol": "http",
                  "port": 8040,
                  "path": "/eus/v1/",
                  "definition": {
                  "type": "openapi",
                  "path": "/eus/v1/api.yaml"
                  }
               },
               {
                  "name": "eusWS",
                  "protocol": "ws",
                  "port": 8040,
                  "path": "/eus/v1/eus-ws"
               }
            ],
            "gui": {
               "protocol": "angular",
               "path": "app-elastest-eus"
            }
         }
      }
   }
